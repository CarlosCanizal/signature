<h1>Listing form_fields</h1>

<table>
  <tr>
    <th>Form</th>
    <th>Field</th>
    <th>Axis x</th>
    <th>Axis y</th>
    <th>Width</th>
    <th>Height</th>
    <th>Page</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @form_fields.each do |form_field| %>
  <tr>
    <td><%= form_field.form_id %></td>
    <td><%= form_field.field_id %></td>
    <td><%= form_field.axis_x %></td>
    <td><%= form_field.axis_y %></td>
    <td><%= form_field.width %></td>
    <td><%= form_field.height %></td>
    <td><%= form_field.page %></td>
    <td><%= link_to 'Show', form_field %></td>
    <td><%= link_to 'Edit', edit_form_field_path(form_field) %></td>
    <td><%= link_to 'Destroy', form_field, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Form field', new_form_field_path %>
